{
  "name": "ts-react-redux-startup",
  "version": "1.0.0",
  "description": "TypeScript redux configuration project",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "build-start": "npm run buildonly:dev && npm start",
    "dev": "supervisor -w dist index.js",
    "build": "rimraf dist && webpack --env production --config webpack.config.production.js",
    "build:dev": "rimraf dist && webpack --env development --config webpack.config.development.js",
    "build:dev:watch": "webpack -w --env development --config webpack.config.development.js",
    "test": "mocha dist/spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brunolm/ts-react-redux-startup"
  },
  "keywords": [
    "typescript",
    "react",
    "redux"
  ],
  "author": {
    "name": "Bruno Leonardo Michels",
    "email": "brunolm7@gmail.com",
    "url": "http://blog.codingwise.com/"
  },
  "license": "MIT",
  "dependencies": {
    "clone": "^1.0.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "history": "^3.0.0",
