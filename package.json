    "@types/react": "^0.14.27",
    "@types/react-dom": "^0.14.14",
    "@types/react-redux": "^4.4.28",
    "@types/react-router": "^2.0.29",
    "@types/redux": "^3.3.26",
    "@types/redux-thunk": "^2.0.27",
    "@types/webpack": "^1.12.30",
